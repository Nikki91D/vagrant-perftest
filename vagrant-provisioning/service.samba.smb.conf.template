[global]
   workgroup = workgroup
   server string = vagrant
   log file = /var/log/samba/log.%m
   max log size = 1000
   syslog = 0
   panic action = /usr/share/samba/panic-action.%d
   server role = standalone

[vagrant]
   comment = home Directory of user vagrant
   path = /home/vagrant
   browseable = yes
   read only = no
   guest ok = no
   valid users = vagrant
